//Используя Intelij IDEA создать проект, пакет. Создать класс Zoo.
//В классе создать список, в который записать 8 животных через метод
//add(index, element). Вывести список в консоль.


import java.util.ArrayList;

public class Zoo {


    public static void main(String[] args) {
        ArrayList arrAnimal = new ArrayList();
        arrAnimal.add(0, "cat");
        arrAnimal.add(1, "dog");
        arrAnimal.add(2, "pig");
        arrAnimal.add(3, "cow");
        arrAnimal.add(4, "parrot");
        arrAnimal.add(5, "hamster");
        arrAnimal.add(6, "mouse");
        arrAnimal.add(7, "rat");
        System.out.println(arrAnimal);

    }
}
