public class Car {
    int year;
    String color;

    public Car() {
    }
    public Car(int year) {
        this.year = year;
    }
    public Car(int year, String color) {
        this.year = year;
        this.color = color;
    }

    public static void main(String[] args){
        Car car1 = new Car();
        Car car2 = new Car(2011);
        Car car3 = new Car(2015, "Синий");
    }
}
