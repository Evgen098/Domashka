public class GoodPupil extends Pupil{

    @Override
    public void study(){
        System.out.println("Ученик хорошо учится");
    }

    @Override
    public void read(){
        System.out.println("Ученик хорошо читает");
    }

    @Override
    public void write(){
        System.out.println("Ученик хорошо пишет");
    }

    @Override
    public void relax(){
        System.out.println("Ученик в меру отдыхает");
    }
}
